
{
  "name": "dwenzel/reporter-api",
  "description": "Reporting API for PHP applications",
  "keywords": [
    "audit",
    "report"
  ],
  "license": [
    "GPL-2.0+"
  ],
  "authors": [
    {
      "name": "Dirk Wenzel",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "cpsit/auditor": "~0.4",
    "web-token/jwt-framework": "^2.0 || ^3.1",
    "myclabs/php-enum": "^1.7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^5.6 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "DWenzel\\ReporterApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DWenzel\\ReporterApi\\Tests\\": "tests"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  }
}
